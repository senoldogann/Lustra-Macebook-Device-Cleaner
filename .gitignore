build/
DerivedData/
*.xcodeproj/xcuserdata/
*.xcworkspace/xcuserdata/
*.xcodeproj/project.xcworkspace/xcuserdata/

# ============================================
# Xcode User State (DO NOT COMMIT!)
# ============================================
*.xcuserstate
*.xccheckout
*.xcscmblueprint
*.hmap
*.ipa
*.dSYM.zip
*.dSYM

# ============================================
# Swift Package Manager
# ============================================
.build/
Packages/
xcuserdata/
Package.resolved
.swiftpm/
*.xcworkspace/xcshareddata/swiftpm/

# ============================================
# CocoaPods (if used)
# ============================================
Pods/
*.xcworkspace
Podfile.lock

# ============================================
# Carthage (if used)
# ============================================
Carthage/Build/
Carthage/Checkouts/

# ============================================
# macOS System Files
